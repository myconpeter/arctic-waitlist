<% if(typeof errors!= 'undefined') { %> <% errors.forEach((error) => { %>

<p class="text-center text-red-600 text-xs font border-solid"><%= error.msg %></p>

<% }) %> <% } %> <% if(success_msg!= '') { %>
<p class="text-center text-red-600 text-xs font border-solid border-green-600 bg-green-600">
	<%=success_msg %>
</p>
<% } %> <% if(error_msg != '') { %>
<p class="text-center text-xs text-red-600 font border-solid"><%= error_msg %></p>
<% } %> <% if(error != '') { %>
<p class="text-center text-red-600 text-xs font border-solid"><%= error %></p>
<% } %>
